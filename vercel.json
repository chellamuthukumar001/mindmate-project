{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/chat",
            "destination": "/api/chat"
        },
        {
            "source": "/symptoms",
            "destination": "/api/symptoms"
        },
        {
            "source": "/health",
            "destination": "/api/health"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}